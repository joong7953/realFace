                <div name="answerDescDiv">
	            </div>

            </div>
	        <% if(userType == 'professor'){ %>
	        	<div class="wrap_btn">
		        <% if(questionNo > 1){ %>
	            	<a href="javascript:prevQuestion('<%=questionNo %>');" class="ui basic button fl"><i class="long arrow left icon"></i>이전문항</a>
	            <% } %>
	            <% if(questionNo < questionCount){ %>
	            	<a href="javascript:nextQuestion('<%=questionNo %>');" class="ui basic button fr">다음문항<i class="long arrow right icon"></i></a>
	            <% } %>
	            </div>
	        <% } %>
        </div>
    </div>