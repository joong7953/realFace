            	<% } %>
            	<% if(fileFullPath3 != ""){ %>
            		<img class="ui centered big image" src="<%=fileFullPath3%>" />
            	<% } %>
                <div class="ui attached message">
                    <div class="header"><%-questionContents%></div>
                </div>
                <div class="ui bottom attached segment">
                	<%
	            		if(1 <= exampleCnt){
	            		
	            	%>
	                    <div class="field" name="example1">
	                        <div class="ui checkbox">
	                            <input type="radio" name="CheckAnswer" tabindex="0" class="hidden" value="1">
	                            <label class="question num01"><%-example1%></label>
	                        </div>
	                        <span class="persent" name="persent1"></span>
	                    </div>
	                <%
	                	}
